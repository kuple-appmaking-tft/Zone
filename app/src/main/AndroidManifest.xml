<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kuple.zone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup">
        <activity android:name=".board.MealWebViewActivity"> </activity>
        <activity android:name=".board.WebViewActivity"> </activity>
        <activity android:name=".chat.ChatRoomActivity"> </activity>
        <activity
            android:name=".board.CorpActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".board.WriteActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".board.DetailActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".board.PhotoboardActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".board.SearchActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".login.EmailCheckActivity"
            tools:ignore="MissingClass" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".login.FindActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".login.SignupActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar.Bridge" />
            tools:ignore="MissingClass" />
        <activity android:name=".timetable.TimeTablePlusActivity" />
        <activity
            android:name=".board.CommonboardActivity"
            tools:ignore="MissingClass" />
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar.Bridge"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".FirebaseCloudMessage.MyFirebaseMessagingService"
            android:exported="false"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <uses-permission android:name="android.permission.INTERNET" />

</manifest>